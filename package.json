{
  "name": "Angular2-Starter",
  "version": "0.0.1",
  "description": "Angular2-Starter",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "build-server": "webpack --config webpack-server.config.js",
    "build-prod-server": "webpack --config webpack-server-prod.config.js",
    "prestart": "npm run build-server",
    "server": "node dist/server/bundle.js -dev",
    "start": "npm run server",
    "build:release": "npm run build-prod-server && gulp build:release",
    "prodserver": "node server/bundle.js",
    "startprod": "npm run prodserver",
    "install:release": "npm install --prefix .release --production",
    "build:ci": "webpack --config webpack.ci.config.js --progress --profile --colors --display-error-details --display-cached",
    "compileE2e": "gulp compileE2e",
    "cleanE2e": "gulp cleanE2e",
    "e2e": "npm run compileE2e && webdriver-manager update && protractor protractor.conf.js && npm run cleanE2e",
    "test": "./node_modules/.bin/karma start"
  },
  "repository": {
    "type": "git"
  },
  "author": "dene",
  "license": "ISC",
  "dependencies": {
    "angular2-universal": "^0.98.3",
    "connect-history-api-fallback": "^1.2.0",
    "express": "^4.13.3",
    "http": "0.0.0",
    "nconf": "^0.8.4",
    "ng2-select": "^1.0.1-beta.0",
    "preboot": "^2.0.7",
    "querystring": "^0.2.0",
    "swig": "^1.4.2",
    "uglify-js": "^2.6.2",
    "webpack-merge": "^0.12.0"
  },
  "devDependencies": {
    "@ngrx/devtools": "^1.3.3",
    "@ngrx/store": "^1.3.3",
    "angular2": "2.0.0-beta.15",
    "bootstrap-sass": "^3.3.6",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-prebuilt": "^0.37.7",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tsc": "^1.1.4",
    "gulp-webpack": "^1.5.0",
    "gulp-zip": "^3.1.0",
    "html-webpack-plugin": "^2.7.1",
    "jasmine-core": "^2.4.1",
    "jasmine-expect": "^2.0.0-beta4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-electron-launcher": "^0.1.0",
    "karma-es6-shim": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.8",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "ng2-bootstrap": "1.0.13",
    "ng2-file-upload": "1.0.0-beta.0",
    "node-sass": "^3.4.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "typescript": "^1.8.7",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-livereload-plugin": "^0.4.0",
    "zone.js": "^0.6.6"
  }
}
